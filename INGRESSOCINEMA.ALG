Algoritmo "IngressoCinema"

Var
    dinheiro, valor_ingresso:real


Inicio
  escreval("Digite quanto dinheiro você tem: ")
  leia(dinheiro)
  
   escreval("Digite o valo do ingresso: ")
   leia(valor_ingresso)
   
   se(dinheiro >=valor_ingresso)entao
   escreval("você pode entrar no cinema.")
   senao
   escreval("Você não pode entrar no cinema.")
   fimse


Fimalgoritmo