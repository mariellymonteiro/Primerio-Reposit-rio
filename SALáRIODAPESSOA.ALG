Algoritmo "Saláriodapessoa"

Var
 salario:real
 dependente:inteiro


Inicio
 escreval("digite o salário da pessoa: ")
  leia (salario)
  escreval("digite o numero de dependentes: ")
  leia(dependente)
  escreval("o salario informado foi: r$ ",salario)
  escreval("o  numero de dependentes e: ",dependente)


Fimalgoritmo